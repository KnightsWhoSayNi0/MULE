;-------------------------------------------------------------------------
; Signed Byte Multiply
; Input:
;	Operand1: 
;                 A : 8 bit 
;
;	Operand2: 
;                 B : 8 bit 
;
; Output:
;	Result:   A : 8 bit product
; Registers Used:
;	none
;-------------------------------------------------------------------------
        assume  adl=1

	public	__bmuls
__bmuls:
	push	bc
	ld	c,a
	mlt	bc
	ld	a,c
	pop	bc
	ret
